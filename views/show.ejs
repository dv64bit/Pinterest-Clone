<% include ./partials/header.ejs %>
    <div class="w-full min-h-screen bg-zinc-950 px-10 py-8 text-white">
        <div class="cards flex flex-wrap gap-10">
            <% loggedInUser.postId.forEach(element=> { %>
                <div class="card w-32">
                    <a href="#" class="inline-block text-xl font-semibold mt-3">
                        <div class="w-full h-40 bg-sky-200 rounded-lg overflow-auto">
                            <img src="/images/uploads/<%= element.newPostedImage %> " class="w-full h-full object-cover"
                                src="" alt="">
                        </div>
                        <h5 class="mt-2 text-sm">
                            <%= element.postTitle %>
                                <h5 class="text-sm font-medium opacity-65">
                                    <%= element.postCaption %>
                                </h5>
                        </h5>
                    </a>
                </div>
                <% }); %>

        </div>
    </div>
    <% include ./partials/footer.ejs %>